    principal: t.Optional[str],
    package: str,
    credential_use: int = 2,
    auth_data: t.Optional[_AuthIdentityBase] = None,
) -> Credential: ...
def decrypt_message(
    context: SecurityContext,
    message: SecBufferDesc,
    seq_no: int = 0,
) -> int: ...
def encrypt_message(
    context: SecurityContext,
